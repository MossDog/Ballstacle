;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Ball"
run/main_scene="res://Scenes/Menu.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")

[autoload]

CsgToolkitAutoload="*res://addons/csg_toolkit/scripts/csg_toolkit_config.gd"

[display]

window/size/viewport_width=2560
window/size/viewport_height=1440
window/size/mode=2
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/csg_toolkit/plugin.cfg", "res://addons/csgtomesh/plugin.cfg", "res://addons/proto_shape/plugin.cfg")

[filesystem]

import/quality/use_taa=true
import/quality/screen_space_aa=1
import/quality/msaa_3d=3
import/quality/msaa_2d=3
import/size/viewport_height=1080
import/size/viewport_width=1920
import/size/initial_position_type=2
import/size/mode=2

